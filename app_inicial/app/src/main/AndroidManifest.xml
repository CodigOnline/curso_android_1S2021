<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.codigonline.cursokotlin">

    <uses-permission android:name="com.android.alarm.permission.SET_ALARM" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_codigonline"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_codigonline_round"
        android:supportsRtl="true"
        android:theme="@style/Base.MyTheme">
        <activity android:name=".NavActivity"></activity>
        <activity android:name=".ui.B.BActivity" />
        <activity android:name=".ui.A.AActivity" />
        <activity android:name=".ui.Planeta.PlanetaActivity" />
        <activity android:name=".ui.RegistroActivity" />
        <activity
            android:name=".ui.MainActivity"
            android:theme="@style/Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>